{
  "name": "Forewarned",
  "version": "1.0.56",
  "slug": "forewarned",
  "description": "Weather alerting and EOC state monitoring with automated routine activation",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "map": ["config:rw"],
  "options": {
    "check_interval": 300,
    "location": "Townsville",
    "eoc_urls": [],
    "voip_enabled": false,
    "voip_backend": "webhook",
    "voip_sip_server": "",
    "voip_sip_user": "",
    "voip_sip_password": "",
    "voip_sip_domain": "",
    "voip_sip_port": 5060,
    "voip_webhook_url": "",
    "voip_webhook_method": "POST",
    "voip_webhook_auth_type": "none",
    "voip_webhook_username": "",
    "voip_webhook_password": "",
    "voip_webhook_token": "",
    "voip_alert_numbers": [],
    "voip_tts_voice": "en-us",
    "voip_tts_speed": 160,
    "mqtt_enabled": false,
    "mqtt_broker": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_username": "",
    "mqtt_password": ""
  },
  "schema": {
    "check_interval": "int",
    "location": "str",
    "eoc_urls": ["str"],
    "voip_enabled": "bool",
    "voip_backend": "list(webhook|sip|ha_notify)",
    "voip_sip_server": "str?",
    "voip_sip_user": "str?",
    "voip_sip_password": "password?",
    "voip_sip_domain": "str?",
    "voip_sip_port": "int(0,65535)?",
    "voip_webhook_url": "str?",
    "voip_webhook_method": "list(GET|POST|PUT)?",
    "voip_webhook_auth_type": "list(none|basic|bearer)?",
    "voip_webhook_username": "str?",
    "voip_webhook_password": "password?",
    "voip_webhook_token": "password?",
    "voip_alert_numbers": ["str"],
    "voip_tts_voice": "list(en-us|en-gb|en-au|en-sc)?",
    "voip_tts_speed": "int(80,450)?",
    "mqtt_enabled": "bool?",
    "mqtt_broker": "str?",
    "mqtt_port": "port?",
    "mqtt_username": "str?",
    "mqtt_password": "password?"
  },
  "ports": {
    "5000/tcp": 5000
  },
  "webui": "http://[HOST]:[PORT:5000]",
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:weather-lightning"
}
